<?xml version="0.0" ?>
<robot name="hanger">

  <link name="base_link">
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0"/>
       <mass value=".0"/>
       <inertia ixx="0" ixy="0" ixz="0" iyy="0" iyz="0" izz="0"/>
    </inertial>
  </link>

  <link name="hanger">
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0"/>
       <mass value=".0"/>
       <inertia ixx="0" ixy="0" ixz="0" iyy="0" iyz="0" izz="0"/>
    </inertial>
    <visual>
      <origin rpy="1.57 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://uwds3/models/cad_models/obj/AlvarAalto106.obj" scale="0.001 0.001 0.001"/>
      </geometry>
       <material name="wood">
        <color rgba="0.572 0.427 0.152 1"/>
      </material>
    </visual>
  </link>

  <joint name="base_to_hanger" type="revolute">
    <parent link="base_link"/>
    <child link="hanger"/>
    <axis xyz="0 0 1"/>
    <limit effort="1000.0" lower="0.0" upper="1.57" velocity="0.5"/>
    <origin rpy="0 0 0" xyz="0 0 0"/>
  </joint>



</robot>
